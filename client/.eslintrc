{
  "parser": "@babel/eslint-parser",
  "extends": [
    "airbnb"
  ],
  "rules": {
    "no-shadow": "off",
    "prefer-const": "off",
    "no-param-reassign": "off",
    "no-alert": "off"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "requireConfigFile": false
  }
}